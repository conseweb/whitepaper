{
    "bookname": "信链白皮书",
    "description": "信链区块链白皮书",
    "language": "zh-hans",
    "output.name": "信链白皮书",
    "direction": "rtl",
    "root": ".",
    "keywords": [
        "信链",
        "bitcoin",
        "conseweb",
        "blockchain",
        "区块链"
    ],
    "links" : {
        "sidebar" : {
            "ICO页面" : "http://www.conseweb.com"
        }
    },

    "homepage": "http://conseweb.github.io/market-research",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/conseweb/market-research.git"
    },
    "bugs": {
        "url": "https://github.com/conseweb/market-research/issues"
    },
    "plugins": [
        "block-align",
        "katex",
        "splitter",
        "-lunr", 
        "-search", 
        "search-plus",
        "simple-page-toc"
    ],
    "pluginsConfig": {
        "anchor-navigation-ex": {
            "isRewritePageTitle": true,
            "isShowTocTitleIcon": true,
            "tocLevel1Icon": "fa fa-hand-o-right",
            "tocLevel2Icon": "fa fa-hand-o-right",
            "tocLevel3Icon": "fa fa-hand-o-right"
        },
        "simple-page-toc": {
            "maxDepth": 3,
            "skipFirstH1": true
        }
    }
}
